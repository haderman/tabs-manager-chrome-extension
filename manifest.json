{
  "name": "Tabs Manager",
  "version": "1.0",
  "description": "Manage your tabs with workspaces",
  "permissions": ["sessions", "tabs", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}